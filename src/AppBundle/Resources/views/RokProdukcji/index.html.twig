{% extends '::base.html.twig' %}

{% block body -%}
    <h1>RokProdukcji list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Idrokprodukcji</th>
                <th>Rok</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('rokprodukcji_show', { 'id': entity.idrokprodukcji }) }}">{{ entity.idrokprodukcji }}</a></td>
                <td>{% if entity.rok %}{{ entity.rok }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('rokprodukcji_show', { 'id': entity.idrokprodukcji }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('rokprodukcji_edit', { 'id': entity.idrokprodukcji }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('rokprodukcji_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
